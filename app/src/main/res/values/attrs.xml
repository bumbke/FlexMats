    <attr name="primaryColor" format="reference" />
    <attr name="secondColor" format="reference" />
    <attr name="accentColor" format="reference" />
    <attr name="primaryTextColor" format="reference" />
    <attr name="secondTextColor" format="reference" />
    <attr name="thirdTextColor" format="reference" />
    <attr name="fourthText" format="reference" />
    <attr name="hintTextColor" format="reference" />
    <attr name="toolbarColor" format="reference" />
    <attr name="background" format="reference" />
    <attr name="primaryiconColor" format="reference" />
    <attr name="secondiconColor" format="reference" />
    <attr name="thirdiconColor" format="reference" />
    <attr name="buttonColor" format="reference" />
    <attr name="secondbuttonColor" format="reference" />
